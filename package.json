{
  "name": "svelte-sapper-starter",
  "description": "svelte-sapper-starter",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "cross-env NODE_ENV=production sapper build --legacy",
    "export": "cross-env NODE_ENV=production sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run",
    "lint:prettier": "prettier --write --loglevel warn",
    "lint:eslint": "eslint --fix",
    "lint:stylelint": "stylelint --fix",
    "lint:all:prettier": "npm run lint:prettier \"**/*.{js,json,css,svelte,html,md}\"",
    "lint:all:eslint": "npm run lint:eslint -- --ext .js,.svelte .",
    "lint:all:stylelint": "npm run lint:stylelint \"src/**/*.{svelte,css}\"",
    "lint": "run-s lint:all:*"
  },
  "dependencies": {
    "compression": "^1.7.1",
    "polka": "next",
    "sirv": "^0.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@rollup/plugin-alias": "^3.0.0",
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "bootstrap": "^4.4.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.26",
    "postcss-import": "^12.0.1",
    "prettier": "^1.19.1",
    "prettier-plugin-svelte": "^0.7.0",
    "rollup": "^1.20.0",
    "rollup-plugin-babel": "^4.0.2",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^5.1.3",
    "sapper": "^0.27.0",
    "stylelint": "^12.0.1",
    "stylelint-config-prettier": "^8.0.0",
    "stylelint-config-recess-order": "^2.0.3",
    "stylelint-config-standard": "^19.0.0",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^3.3.0"
  }
}
